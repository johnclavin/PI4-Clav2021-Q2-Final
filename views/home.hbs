{{> header}}
<div class="container">
	<div class="row space-top">
		<div class="col-md-12">
			<h2>Welcome To The First Alien Contact Event</h2>
			<p>Sign Up Here</p>
			<img src="./images/fs6.png" alt="Alien Welcome" width="1000" height="1000">
		</div>
	</div>
	<div class="row space-top">
		{{#each event}}
			<div class="col-md-4">
				<div class="card text-white bg-dark">
					<div class="card-body">
						<blockquote class="card-blockquote">
							<img src="images" />
							<p>Nothing to see here...</p>
							<footer>"Alien Contact"
								<cite title="year">2045</cite>
							</footer>
							<div class="pull-right">
								<a
									href="#/viewOne/{{id}}"
									class="btn btn-info"
								>Details</a>
							</div>
						</blockquote>
					</div>
				</div>
			</div>
		{{/each}}
	</div>
</div>